    "body-parser": "^1.18.2",
    "cors": "^2.8.5",
    "decimal.js": "^10.4.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.12",
    "mongoose-long": "^0.6.0",
    "morgan": "^1.10.0",
    "node-telegram-bot-api": "^0.63.0",
    "passport": "^0.4.0",
    "passport-jwt": "^3.0.0",
    "pg": "^8.11.3",
    "socket.io": "^4.6.1",
    "socket.io-msgpack-parser": "^3.0.2",
